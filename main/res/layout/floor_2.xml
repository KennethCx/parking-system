<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/floor_2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/background2"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.cx.parkingmanagementsystem.RangeView
            android:id="@+id/rangeView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>

    <include
        android:id="@+id/parkingspaceView"
        layout="@layout/floor_2_parkingspace" />

    <include
        android:id="@+id/lightView"
        layout="@layout/floor_2_light" />

    <include
        android:id="@+id/cameraView"
        layout="@layout/floor_2_camera" />

    <include
        android:id="@+id/hydrantView"
        layout="@layout/floor_2_hydrant" />

    <include
        android:id="@+id/floorlightView"
        layout="@layout/floor_2_floorlight" />
</RelativeLayout>
