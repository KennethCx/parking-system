<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/floor_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background1"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.cx.parkingmanagementsystem.widget.RangeView_Light
            android:id="@+id/rangeView_Light"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.example.cx.parkingmanagementsystem.widget.RangeView_Camera
            android:id="@+id/rangeView_Camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>

    <include
        android:id="@+id/parkingspaceView"
        layout="@layout/floor_1_parkingspace" />

    <include
        android:id="@+id/lightView"
        layout="@layout/floor_1_light" />

    <include
        android:id="@+id/cameraView"
        layout="@layout/floor_1_camera" />

    <include
        android:id="@+id/hydrantView"
        layout="@layout/floor_1_hydrant" />

    <include
        android:id="@+id/floorlightView"
        layout="@layout/floor_1_floorlight" />

</RelativeLayout>

